        <services>
            <service api="mn_crud" method="get" rest="object/${GUID}" available="true"    datechecked="2010-06-07T23:55:10Z" />
            <service api="mn_crud" method="getSystemMetadata" rest="meta/${GUID}" available="true"     datechecked="2010-06-07T23:55:10Z" />
            <service api="mn_replicate" method="listObjects" rest="object" available="true"    datechecked="2010-06-07T23:55:10Z" />
        </services>
        <synchronization>
            <schedule sec="00" min="00" hour="12" mday="*"  mon="*" wday="*" year="*" />
            <lastHarvested>2010-06-07T23:55:10Z</lastHarvested>
            <lastCompleteHarvest>2010-06-07T23:55:10Z</lastCompleteHarvest>
        </synchronization>
    </node> 
    
